@model Tuple<object, Int32>
@{
	ViewData["Title"] = "Diagram";
}
<!DOCTYPE html>
<html>
<head>
	<environment names="Development">
		<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-cosmo.css" />
		<link rel="stylesheet" href="~/css/site.css" />
	</environment>
	<environment names="Staging,Production">
		<link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
			  asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
			  asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
		<link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
	</environment>
	<meta name="viewport" content="width=device-width" />

	<script src="~/js/3DViewer/three.js/build/three.min.js"></script>

	<script src="~/js/3DViewer/three.js/controls/TrackballControls.js"></script>
	<script src="~/js/3DViewer/three.js/controls/OrbitControls.js"></script>
	<script src="~/js/3DViewer/three.js/libs/stats.min.js"></script>

	<script src="~/js/3DViewer/libs/lodash.core.js"></script>
	<script src="~/js/3DViewer/libs/sylvester.js"></script>
	<script src="~/js/3DViewer/libs/libtess.min.js"></script>
	<script src="~/js/3DViewer/libs/pnltri.min.js"></script>
	<script src="~/js/3DViewer/libs/points-3d-to-2d.js"></script>
	<script src="~/js/3DViewer/libs/triangulate.js"></script>

	<script src="~/lib/svg-pan-zoom.js"></script>
</head>
<body style="left: 0px; top: 0px;">
	<div class="list-group" id="dropdown_menu" style="position: absolute; z-index: 2;">
		<a class="list-group-item btn">
			<span>Добавить в вид</span>
		</a>
	</div>
	<div class="row">
		<div class="col-md-2 panel panel-default" style="top: 0px; height: 800px; padding-left: 0px; padding-right: 0px; z-index: 1;">
			<div class="panel-heading">
				<h3 class="panel-title">Проект</h3>
			</div>
			<div class="panel-body" id="projectTree" style="width: 100%; height: 730px;">
				@*<iframe id="modelTreeFrame" style="width: 100%; height: 730px;"></iframe>*@
			</div>
		</div>
		<div class="col-md-2 panel panel-default" id="properties" style="height: 800px; top: 0px; padding-left: 0px; padding-right: 0px; z-index: 1;">
			<div class="panel-heading">
				<h3 class="panel-title" id="h1">Свойства</h3>
			</div>
			<div class="panel-body" id="p1">
			</div>
		</div>
		<div id="mainPage" class="col-md-4" style="padding-left: 0px; padding-right: 0px;">
			<div class="model" id="model_container" style="padding-left: 0px; padding-top: 0px; z-index:0; width: 800px; height: 400px; border: 1px solid #dddddd;"></div>
			<div id="container" style="width: 800px; height: 400px; border: 1px solid #dddddd;">
				@Html.Raw(Model.Item1)
			</div>
			<button type="button" class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg">
				Опросный лист
			</button>
		</div>

		<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
			<div class="modal-dialog modal-lg" role="document">
				<div class="modal-content">
					@*<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
							<h4 class="modal-title">Опросный лист</h4>
						</div>*@
					<div class="modal-body">
						<div id="datasheet" style="position: absolute; padding-left: 0px; padding-top: 0px; z-index:0; width: 1000px; border: 1px solid #dddddd;">
							<IMG SRC="~/SupportFiles/ОЛ_РегКлапан.png" ALT="some text" style="width: 100%;">
						</div>
						<div id="datasheet_grid" style="position: absolute; padding-left: 0px; padding-top: 0px; z-index:1; width: 800px; border: 1px solid #dddddd;">
							<input style="position: absolute; border: 1px solid #000000; top: 157px; left: 547px; font-size: 10px; font-weight: bold; text-align: center; width: 300px;" />
						</div>
					</div>
					@*<div class="modal-footer">
							<button type="button" class="btn btn-default" data-dismiss="modal">Закрыть</button>
						</div>*@
				</div>
			</div>
		</div>

	</div>
	<div style="position:absolute; background-color:antiquewhite" id="hint"></div>

	<environment names="Development">
		<script src="~/lib/jquery/dist/jquery.js"></script>
	</environment>
	<environment names="Staging,Production">
		<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
				asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
				asp-fallback-test="window.jQuery">
		</script>
	</environment>

	<script>
        var modelGeometriesArraySize = @Model.Item2;
	</script>
	<script src="~/js/3DViewer/main.js"></script>
</body>
</html>